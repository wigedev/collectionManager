{
  "name": "wigedev/jasper",
  "description": "A basic project template to enable installing Jasper with Composer",
  "license": "Apache-2.0",
  "type": "project",
  "authors": [
    {
      "name": "William Eldridge",
      "homepage": "http://wigedev.net"
    }
  ],
  "require": {
    "php": ">=7.1",
    "psr/log": "1.0.*",
    "wigedev/jasper-core": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Application\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "tests/"
    }
  }
}